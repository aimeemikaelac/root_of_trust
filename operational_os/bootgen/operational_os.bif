the_ROM_image:
{
[keysrc_encryption] bbram_red_key
[fsbl_config] a53_x64, auth_only, bh_auth_enable
[auth_params] ppk_select=0; spk_id=0x00000000
[pskfile] /mnt/sdcard//DEVICE_AUTHENTICATION_KEY.pem
[sskfile] /mnt/sdcard//SSK.pem
[bootloader, authentication=rsa] bootgen/fsbl_aes_custom.elf
[destination_cpu=a53-0, exception_level=el-3, trustzone, authentication=rsa] bootgen/bl31.elf
[destination_cpu=a53-0, exception_level=el-2, authentication=rsa] bootgen/u-boot-custom.elf
}